<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\AppLogo.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\check.png" />
    <None Remove="Resources\reset.png" />
    <None Remove="Resources\swap.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\AppLogo.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\check.png" />
    <Resource Include="Resources\reset.png" />
    <Resource Include="Resources\swap.png" />
  </ItemGroup>

</Project>
